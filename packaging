packager in=bippop_sd.mp4,stream=audio,init_segment='widevine_video/audio_init.mp4',segment_template='widevine_video/audio_$Number%03d$.m4s' \
in=bippop_sd.mp4,stream=video,init_segment='widevine_video/h264_480p_init.mp4',segment_template='widevine_video/h264_480p_$Number%03d$.m4s' \
in=bippop_hd.mp4,stream=video,init_segment='widevine_video/h264_720p_init.mp4',segment_template='widevine_video/h264_720p_$Number%03d$.m4s' \
in=bippop_full_hd.mp4,stream=video,init_segment='widevine_video/h264_1080p_init.mp4',segment_template='widevine_video/h264_1080p_$Number%03d$.m4s' \
--mpd_output h264.mpd
